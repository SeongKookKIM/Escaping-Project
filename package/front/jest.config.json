{
  "roots": ["<rootDir>"],
  "testEnvironment": "jsdom",
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy",
    "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/src/test/fileMock.ts"
  },
  "setupFilesAfterEnv": ["<rootDir>/src/test/jest-setup.ts"],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  }
}
